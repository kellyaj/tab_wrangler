{
  "manifest_version": 2,
  "name": "Tab Wrangler",
  "description": "The Tab Wrangler",
  "version": "0.0.1",
  "chrome_url_overrides" : { "newtab": "./build/html/wrangler.html" },
  "background": {
    "scripts": ["./build/js/bundles/background_bundle.js"],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "storage",
    "alarms"
  ]
}
